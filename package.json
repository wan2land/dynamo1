{
  "name": "dynamo1",
  "version": "0.1.4",
  "description": "Dynamo DB one table ORM for Javascript(& Typescript).",
  "keywords": [
    "dynamodb",
    "database",
    "orm",
    "typescript"
  ],
  "author": "Changwan Jun <wan2land@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/wan2land/dynamo1.git"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepack": "npm run test && npm run build",
    "build": "rimraf ./lib && tsc --emitDeclarationOnly && babel src --out-dir lib --extensions \".ts\" --source-maps inline",
    "test": "npm run build && npm run test:lint && npm run test:unit",
    "test:lint": "eslint src/**/*",
    "test:unit": "jest test"
  },
  "dependencies": {
    "aws-sdk": "^2",
    "core-js": "^3.0.0",
    "relater": "^0.2.0",
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.10.1",
    "@babel/core": "7.10.1",
    "@babel/plugin-proposal-class-properties": "7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "7.10.1",
    "@babel/preset-env": "7.10.1",
    "@babel/preset-typescript": "7.10.1",
    "@types/faker": "4.1.12",
    "@types/jest": "25.1.3",
    "@types/node": "11.15.7",
    "@types/uuid": "8.0.0",
    "@typescript-eslint/eslint-plugin": "3.0.1",
    "@typescript-eslint/parser": "3.0.1",
    "eslint": "7.1.0",
    "eslint-config-stable": "^0.7.4",
    "faker": "4.1.0",
    "jest": "26.0.1",
    "rimraf": "3.0.2",
    "ts-jest": "26.0.0",
    "typescript": "3.9.5"
  }
}
