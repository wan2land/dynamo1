{
  "name": "dynamo1",
  "version": "0.5.0",
  "description": "Dynamo DB one table ORM for Javascript(& Typescript).",
  "keywords": [
    "dynamodb",
    "database",
    "orm",
    "typescript"
  ],
  "author": "Changwan Jun <wan2land@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/wan2land/dynamo1.git"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepack": "npm ci && npm run test && npm run build",
    "build": "rimraf ./lib && tsc -p tsconfig.build.json && babel src --out-dir lib --extensions \".ts\"",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "eslint src/**/* test/**/*",
    "test:unit": "jest test"
  },
  "dependencies": {
    "aws-sdk": "2",
    "core-js": "3.6.5"
  },
  "devDependencies": {
    "@babel/cli": "7.12.1",
    "@babel/core": "7.12.3",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "7.12.1",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-typescript": "7.12.1",
    "@types/faker": "5.1.2",
    "@types/jest": "26.0.15",
    "@types/node": "12.19.3",
    "@types/uuid": "8.3.0",
    "eslint": "7.12.1",
    "eslint-config-stable": "0.8.0",
    "faker": "5.1.0",
    "jest": "26.6.1",
    "mockdate": "3.0.2",
    "rimraf": "3.0.2",
    "ts-jest": "26.4.3",
    "typescript": "4.0.5",
    "uuid": "8.3.1"
  }
}
