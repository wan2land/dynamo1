{
  "name": "dynamo1",
  "version": "0.1.4",
  "description": "Dynamo DB one table ORM for Javascript(& Typescript).",
  "keywords": [
    "dynamodb",
    "database",
    "orm",
    "typescript"
  ],
  "author": "Cris <corgidisco@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/corgidisco/dynamo1.git"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepack": "npm run test && npm run build",
    "build": "rimraf ./lib && tsc --emitDeclarationOnly && babel src --out-dir lib --extensions \".ts\" --source-maps inline",
    "test": "npm run build && npm run test:lint && npm run test:unit",
    "test:lint": "eslint src/**/*",
    "test:unit": "jest test"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "[^/]*\\.test.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "dependencies": {
    "aws-sdk": "^2",
    "core-js": "^3.0.0",
    "relater": "^0.2.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.4.0",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "7.4.0",
    "@babel/preset-env": "7.4.2",
    "@babel/preset-typescript": "7.3.3",
    "@types/faker": "4.1.5",
    "@types/jest": "24.0.11",
    "@types/node": "11.11.4",
    "@types/uuid": "3.4.4",
    "@typescript-eslint/eslint-plugin": "1.5.0",
    "eslint": "5.16.0",
    "faker": "4.1.0",
    "jest": "24.5.0",
    "rimraf": "2.6.3",
    "ts-jest": "24.0.0",
    "typescript": "3.3.4000"
  }
}
